{
  "dataset": "CIFAR10",
  "network": "swin_t",
  "optimizer": "kfac",
  "scheduler": "CosineAnnealingLR",
  "init_lr": 0.001,
  "early_stop_threshold": -1.0,
  "optimizer_kwargs": {
    "weight_decay": 0.01,
    "momentum": 0.9,
    "damping": 0.001,
    "curvature_update_interval": 100.0,
    "ema_decay": -1.0,
    "clipping_norm": 10.0
  },
  "scheduler_kwargs": {
    "T_max": 192.0
  },
  "cutout": false,
  "n_holes": 1,
  "cutout_length": 16,
  "n_trials": 1,
  "num_workers": 5,
  "max_epochs": 192,
  "mini_batch_size": 256,
  "loss": "cross_entropy",
  "early_stop_patience": 10
}