{
  "dataset": "CIFAR100",
  "network": "resnet101Cifar",
  "optimizer": "AdaFisher",
  "scheduler": "CosineAnnealingLR",
  "init_lr": 0.01,
  "early_stop_threshold": -1.0,
  "optimizer_kwargs": {
    "weight_decay": 0.0005,
    "beta3": 0.91,
    "Lambda": 0.001,
    "TCov": 10.0
  },
  "scheduler_kwargs": {
    "T_max": 200.0
  },
  "cutout": false,
  "n_holes": 1,
  "cutout_length": 16,
  "n_trials": 1,
  "num_workers": 16,
  "max_epochs": 200,
  "mini_batch_size": 256,
  "loss": "cross_entropy",
  "early_stop_patience": 10
}